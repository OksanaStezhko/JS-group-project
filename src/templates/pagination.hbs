               <li class="pagination__item prev"></li>
               {{#each this}}
                     {{#if this.isActive }}
                         <li class="pagination__item active">{{this.elem}}</li>
                     {{else}}
                         <li class="pagination__item ">{{this.elem}}</li>
                     {{/if}}
               {{/each}}
               <li class="pagination__item next"></li>


